  @keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(20px); /* Elements start 20px below their final position */
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

main > p, main > svg {
    animation-name: fadeInUp;
    animation-duration: 1s; /* Adjust the duration as needed */
    animation-fill-mode: both; /* Keeps the element in its final state after the animation ends */
}

main > p:nth-child(1) {
    animation-delay: 0.8s; /* Starts half a second after the page loads */
}

main > p:nth-child(2) {
    animation-delay: 0.6s; /* Starts 1 second after the page loads */
}

